    <record id="account_tax_template_s_igi45" model="account.tax.template">
        <field name="sequence" eval="0"/> <!-- Impost per defecte a la venda -->
        <field name="description"/> <!-- for resetting the value on existing DBs -->
        <field name="type_tax_use">sale</field>
        <field name="name">IGI 4,5% Vendes</field>
